{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "forOf": true,
    "jsx": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 0,
    "react/prop-types": 0,
    "react/prefer-es6-class": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-indent-props": 0
  }
}
